{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2015",
    "sourceMap": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "jsx": "preserve",
    "allowJs": false,
    "isolatedModules": true,
    "types": [
      "vite/client",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": ["ESNext"]
  },
}
